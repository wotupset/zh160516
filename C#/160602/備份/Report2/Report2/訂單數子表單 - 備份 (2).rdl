        <Field Name="金額">
          <DataField>金額</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DATEPART(YEAR,訂單日期) AS 年,
COUNT(*) AS 訂單數,
ROUND(SUM(單價*數量*(1-折扣)),0) AS 金額
FROM 訂貨主檔 JOIN 訂貨明細 ON 訂貨主檔.訂單號碼=訂貨明細.訂單號碼
WHERE 客戶編號=@CID
GROUP BY DATEPART(YEAR,訂單日期)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@CID">
            <Value>=Parameters!CID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
